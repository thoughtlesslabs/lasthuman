function update_menu()
	logo_animation()
	
	if btnp(5) then
		_update60 = update_game
		_draw = draw_game
		tb_init(61,{"sir aaron..."
		,"a bountiful treasure awaits!"
		,"you must find all 16 magic\nscrolls to unlock the treasure."
		,"solve the puzzles to find\nthe scrolls."
		,"go forth!"})
	end
end

function update_game()
 open_hidden()
 calc_code()
 run_anim()
 if reading then
  tb_update()
	else
  if btnp(4) then
  	display_inventory()
  	sfx(58)
 	end
 	if not show_inventory then
  	move_player()
 	else
 		if btnp(5) then
 			restart_level()
 			sfx(58)
 		end
  end
 end
end

function move_player()
	
	newx = p.x
	newy = p.y
	
	if btnp(0) then newx -=1 push="right" end
	if btnp(1) then newx +=1 push="left" end
	if btnp(2) then newy -=1 push="up" end
	if btnp(3) then newy +=1 push="down" end
	
	interact(newx,newy)
	
	if can_move(newx,newy) then
		p.x = mid(0,newx,127)
		p.y = mid(0,newy,63)
	else
		sfx(61)
	end
end
