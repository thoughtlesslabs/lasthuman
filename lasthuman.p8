-- game functions

--  check if the player can move
function can_move(x,y)
	if is_tile(wall,x,y) or is_movable(x,y) then
		return false
	else
		return true
	end
end

-- check for tile type
function is_tile(tile_type,x,y)
	tile = mget(x,y)
	for i=1,#tile_type do
		if tile == tile_type[i] then
			return true
		end
	end
	return false
end

-- check if hit crate
function is_movable(x,y)
	for i=1,#movable_items do
		if x == movable_items[i].x and y==movable_items[i].y then
			return true
		end
	end
	return false
end

-- check if start of puzzle
function is_start(x,y)
	for i=1,#start_locations do
		if x == start_locations[i].x and y==start_locations[i].y then
			return true
		end
	end
	return false
end

-- check if a keycode
function is_keycode(x,y)
	for i=1,#keycode do
		if not keycode[i].show then
			if keycode[i].active and x == keycode[i].x and y == keycode[i].y then
				return true
			end
		end
	end
	return false
end

-- interaction options based
-- on the tile
function interact(x,y)
	if is_tile(door,x,y) then
		if x==5 and y==31 then
			local	missing_scrolls = 0
			for i=1,#keycode do
				if keycode[i].show then
					missing_scrolls +=1
				end
			end
			missing_scrolls = 16-missing_scrolls
			if missing_scrolls >0 then
				tb_init(61,{"you are still missing "..missing_scrolls.."\nmagic scrolls. come back later"})
			else
				tb_init(61,{"you found all the magic scrolls\nyour treasure is inside"})
				swap_tile(x,y)
			end
		elseif p.keys>0 then
		swap_tile(x,y)
		p.keys -=1
	 else
	 	tb_init(61,{"the door is locked"})
	 end
	 sfx(62)
	elseif is_tile(waterwalk,x,y) then
		swap_tile(x,y)
	elseif is_movable(x,y) then
		for i=1,#movable_items do
			if x == movable_items[i].x and y== movable_items[i].y then
				movable_items[i].active = true
			end
		end
		move_item(x,y)
	elseif is_tile(key,x,y) then
		p.keys+=1
		swap_tile(x,y)
		sfx(63)
		if is_keycode(x,y) then
			update_code(x,y)
			extranote = "\nand a piece of the puzzle"
		else
			extranote = ""
		end
		tb_init(61,{"you found a key!"..extranote})
	elseif is_keycode(x,y) then
		sfx(60)
		inventory_clue = ""
		for i=1,#keycode do
			if x == keycode[i].x and y==keycode[i].y and keycode[i].key == "a" then
				inventory_clue="\ncheck your inventory with üÖæÔ∏è"
			end
		end
		tb_init(61,{"you found a magic scroll!\n"..inventory_clue})
		update_code(x,y)
	elseif is_tile(treasure,x,y) then
		gameover = true
		swap_tile(x,y)
		music(26)
	elseif is_tile(lose,x,y) then
		restart_level()
		tb_init(61,{"uh-oh...","you need to watch out for\nthose spikes!"})
		sfx(28)
	end
end
		
-- swap and unswap tiles
function swap_tile(x,y)
	tile = mget(x,y)
	mset(x,y,tile+1)
end

function unswap_tile(x,y)
	tile = mget(x,y)
	mset(x,y,tile-1)
end

-- toggle tiles for animations
function toggle_tiles()
	for x=mapx,mapx+15 do
		for y=mapy,mapy+15 do
			if is_tile(anim1,x,y) then
				swap_tile(x,y)
			elseif is_tile(anim2,x,y) then
				unswap_tile(x,y)
			end
		end
	end
end

-- animation timers
function run_anim()
	if timer<0 then
		toggle_tiles()
		timer=anim_time
	end
	timer -=1
end

-- push a crate
function move_item(x,y)

	nx = x
	ny = y
 
	for i=1,#movable_items do
		if movable_items[i].active then
		
			if push == "left" then
				nx +=1
			elseif push == "right" then
			 nx -=1
			elseif push == "up" then
				ny -=1
			elseif push == "down" then
				ny +=1
			end
			if can_move(nx,ny) then
				if not is_keycode(nx,ny) then
					if not is_tile(cavedoor,nx,ny) then
						movable_items[i].x = mid(0,nx,127)
						movable_items[i].y = mid(0,ny,63)
				 end
				end
			end
		movable_items[i].active = false
		end
	end
	sfx(59)
end

-- update found code letters
function update_code(x,y)
	for i=1,#keycode do
		if x == keycode[i].x and y == keycode[i].y then
			if keycode[i].active then
				keycode[i].show = true
				keycode[i].sprite += 2
				mset(keycode[i].x,keycode[i].y,keycode[i].sprite)
			end
		end
	end
end

-- calculate code inventory
function calc_code()
	local chrs={"","","","","","","","","","","","","","","",""}
 for i=1,#keycode do
 	if keycode[i].show then
 	 chrs[i] = keycode[i].key
 	else
 		chrs[i] = " "
 	end
 end
 showcode = chrs[1]
 ..chrs[2]
 ..chrs[3]
 ..chrs[4]
 ..chrs[5]
 ..chrs[6]
 ..chrs[7]
 ..chrs[8]
 ..chrs[9]
 ..chrs[10]
 ..chrs[11]
 ..chrs[12]
 ..chrs[13]
 ..chrs[14]
 ..chrs[15]
 ..chrs[16]
end


-- show/hide inventory
function display_inventory()
	if show_inventory == false then
		show_inventory = true
	else
		show_inventory = false
	end
end

-- open hidden entrances
function open_hidden()
	if is_tile(cavefloor,46,1) then
		mset(47,14,106)
	end
	if is_movable(6,21) then
		for i=1,#keycode do		
		 if keycode[i].x==9 and keycode[i].y==19 then
				keycode[i].active = true
		 end
		end
	end
end 		

-- reset level
function restart_level()
	for x=mapx,mapx+15 do
		for y=mapy,mapy+15 do
			if is_start(x,y) then
				setx = x
 			sety = y
 		end
			for i=#movable_items,1,-1 do
				if movable_items[i].x == x and movable_items[i].y == y then
					movable_items[i].x =  movable_items[i].ox
					movable_items[i].y =  movable_items[i].oy
				end
			end
		end
	end
	p.x = setx
	p.y = sety
 if show_inventory then
	 display_inventory()
 end
end 
	
